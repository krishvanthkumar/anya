<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".doctermenu">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Menu"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.076" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="28dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.861"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.088"
        app:srcCompat="@drawable/menubar" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="413dp"
        android:layout_height="457dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pregwom" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="match_parent"
        android:layout_height="458dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993"
        app:srcCompat="@drawable/menupg_box" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.878">

        <LinearLayout
            android:id="@+id/profile"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="482dp">

            <ImageView
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/human" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="274dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Profile"
                android:textColor="@color/black"
                android:textSize="24sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/upload"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="556dp">

            <ImageView
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/upload" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="275dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Upload Video"
                android:textColor="@color/black"
                android:textSize="24sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/addpatient"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="634dp">

            <ImageView
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/adduser" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="275dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Add Patient"
                android:textColor="@color/black"
                android:textSize="24sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/logout"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="714dp">

            <ImageView
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/logout" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="273dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Logout"
                android:textColor="@color/black"
                android:textSize="24sp" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>